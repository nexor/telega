{
	"name": "telega",
	"dockerFile": "Dockerfile",

	"appPort": [9000],

	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},

	"mounts": [
	],

	"remoteEnv": {
		"APP_ENV": "dev"
	},

	// Comment out the next line to run as root
